<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#f2f2f2" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
            <stroke
                android:color="#fafafa"
                android:width="4dp"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid
                android:color="#a0a7ac"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <!-- 32aeea 919296 06189F -->
    <item>
        <shape>
            <solid
                android:color="#fafafa" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
            <corners android:radius="3dp"/>
            <stroke
                android:color="#a2a2a2"
                android:width="2dp"/>
        </shape>
    </item>
</selector>